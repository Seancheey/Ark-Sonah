<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.seancheey.scene.controller.SettingController">
    <top>
        <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <items>
                <Button mnemonicParsing="false" onAction="#menuButtonPressed" text="Menu"/>
            </items>
        </ToolBar>
    </top>
    <center>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <GridPane alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.rowSpan="2">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <StackPane GridPane.rowIndex="1">
                            <children>
                                <Label text="music" translateY="-10.0" GridPane.rowIndex="1"/>
                            </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="1">
                            <children>
                                <Slider orientation="VERTICAL" GridPane.columnIndex="1"/>
                            </children>
                        </StackPane>
                        <StackPane>
                            <children>
                                <Slider orientation="VERTICAL"/>
                            </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="2">
                            <children>
                                <Slider orientation="VERTICAL" GridPane.columnIndex="2"/>
                            </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="2" GridPane.rowIndex="1">
                            <children>
                                <Label text="master" translateY="-10.0" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                            </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <Label text="sound" translateY="-10.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            </children>
                        </StackPane>
                    </children>
                </GridPane>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowSpan="3">
                    <children>
                        <CheckBox alignment="CENTER" mnemonicParsing="false" nodeOrientation="RIGHT_TO_LEFT"
                                  text="Fullscreen">
                            <font>
                                <Font size="14.0"/>
                            </font>
                            <VBox.margin>
                                <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
                            </VBox.margin>
                        </CheckBox>
                        <HBox alignment="CENTER_LEFT">
                            <children>
                                <Label text="Max Frame Rate"/>
                                <TextField maxWidth="50.0" prefHeight="27.0" prefWidth="50.0"/>
                            </children>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER_LEFT">
                            <children>
                                <Label text="Save Path"/>
                                <DatePicker prefHeight="27.0" prefWidth="131.0"/>
                            </children>
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
                <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"/>
            </children>
        </GridPane>
    </center>
</BorderPane>
