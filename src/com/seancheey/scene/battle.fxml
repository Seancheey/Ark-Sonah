<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="341.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.seancheey.scene.controller.BattleController" stylesheets="@battleStyle.css">
    <children>
        <ToolBar prefHeight="40.0" prefWidth="562.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="0.0">
            <items>
                <Button mnemonicParsing="false" onAction="#menu" text="Menu"/>
                <Button mnemonicParsing="false" onAction="#start" text="Start"/>
                <Button mnemonicParsing="false" onAction="#pause" text="Pause"/>
                <Label fx:id="moneyLabel" alignment="CENTER" ellipsisString="" text="money" textAlignment="CENTER">
                    <padding>
                        <Insets left="10.0" right="10.0"/>
                    </padding>
                </Label>
            </items>
        </ToolBar>
        <HBox fx:id="botGroupBox" alignment="CENTER" prefHeight="60.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
        <StackPane fx:id="battleContainer" layoutX="200.0" layoutY="41.0" maxHeight="1.7976931348623157E308"
                   maxWidth="1.7976931348623157E308" prefHeight="205.0" prefWidth="600.0"
                   AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="40.0"/>
    </children>
</AnchorPane>
